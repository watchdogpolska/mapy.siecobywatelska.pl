{% extends 'SonataBlockBundle:Block:block_base.html.twig' %}

{% block block %}
<div class="box">
  <div class="box-header">
    <h3 class="box-title">{{ settings.title }}</h3>
  </div>
  <div class="box-body">
    <iframe src="{{ settings.url }}" frameborder="0" style="width: 100%; height: {{ settings.height }};"></iframe>
  </div>
</div>
{% endblock %}


